local Bennett = Window:NewTab("Powers")
local BennettSection = Bennett:NewSection("ALL WITCHES!")
--auto things

_G.AutoPunch = true

function AutoPunch()

while _G.AutoPunch == true do
local args = {
    [1] = "Refresh"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("StatCheck"):FireServer(unpack(args))
local args = {
    [1] = "Refresh"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("StatCheck"):FireServer(unpack(args))
local args = {
    [1] = "Refresh"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("StatCheck"):FireServer(unpack(args))
wait(.0000000000000000000000000000000000000000000000000001)
end
end

BennettSection:NewToggle("Auto Restore magic", "Restoring", function(Value)
    print("You just clicked the bind")
		_G.AutoPunch = Value
                AutoPunch()
        end)

BennettSection:NewButton("Self Heal", " ", function()

-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = {
        ["IncantationText"] = "sanitatem",
        ["Target"] = game:GetService("Players").LocalPlayer.Character
    },
    [2] = Vector3.new()
}

game:GetService("ReplicatedStorage").Events.WitchSpell:FireServer(unpack(bennettscripts))


end)
local BennettSection = Bennett:NewSection("Bonnie")

BennettSection:NewTextBox("Incinerate Someone", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character

      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart.CFrame

wait(0.5)
local args = {
    [1] = Enum.KeyCode.Q,
    [2] = player.Character,
    [3] = Vector3.new(-72.59432983398438, 140.65469360351562, 211.61338806152344)
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("KeyBind"):FireServer(unpack(args))
    

end)

BennettSection:NewToggle("Keybinds On/Off", "", function(state)
    if state == true then
        game.workspace["Bonnie Keybinds"].Archivable = true
    else
        game.workspace["Bonnie Keybinds"].Archivable = false
    end
end)

BennettSection:NewKeybind("Combo 1", "",Enum.KeyCode.One, function()
    if game.workspace["Bonnie Keybinds"].Archivable == true then
    local function getPlayer()
            local userInput = game:GetService("UserInputService")
            local cam = workspace.CurrentCamera
            local mousePos = userInput:GetMouseLocation()
            local mouseRay = cam:ViewportPointToRay(mousePos.X, mousePos.Y)
            players = {}
            for _, x in next, game:GetService('Players'):GetPlayers() do
                players[#players + 1] = x.Character
            end
            local raycastParams = RaycastParams.new()
            raycastParams.FilterType = Enum.RaycastFilterType.Whitelist
            raycastParams.FilterDescendantsInstances = players
            raycastParams.IgnoreWater = true
            local raycastResult = workspace:Raycast(mouseRay.Origin, mouseRay.Direction * 1000, raycastParams)
            if raycastResult then
                for _, x in next, game:GetService('Players'):GetPlayers() do
                    if not raycastResult.Instance:IsDescendantOf(x.Character) then
                        continue 
                    end
                    return x
                end
            end
            return false
        end
        local function Set(list)
            local set = {}
            for _, l in ipairs(list) do
                if l then
                    set[l] = true
                end
            end
            return set
        end
        if IsTyping then return end
        local player = getPlayer()
        if player then
            local pos = player.Character.HumanoidRootPart.CFrame

-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = Enum.KeyCode.X,
    [2] = player.Character,
    [3] = Vector3.new()
}

game:GetService("ReplicatedStorage").Events.KeyBind:FireServer(unpack(bennettscripts))

-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = Enum.KeyCode.Z,
    [2] = player.Character,
    [3] = Vector3.new()
}

game:GetService("ReplicatedStorage").Events.KeyBind:FireServer(unpack(bennettscripts))

-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
