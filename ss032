local BonnieKeybinds = Instance.new("Folder",workspace)
BonnieKeybinds.Name = "Bonnie Keybinds"
local FreyaKeybinds = Instance.new("Folder",workspace)
FreyaKeybinds.Name = "Freya Keybinds"
local DahliaKeybinds = Instance.new("Folder",workspace)
DahliaKeybinds.Name = "Dahlia Keybinds"
local CleoKeybinds = Instance.new("Folder",workspace)
CleoKeybinds.Name = "Cleo Keybinds"
wait(0.5)
game.workspace["Cleo Keybinds"].Archivable = false
game.workspace["Dahlia Keybinds"].Archivable = false
game.workspace["Freya Keybinds"].Archivable = false
game.workspace["Bonnie Keybinds"].Archivable = false
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Bennett Scripts | The Witches Prophecies", "GrapeTheme")
local Bennett = Window:NewTab("Characters")

local BennettSection = Bennett:NewSection("Witch")

BennettSection:NewButton("Bonnie", " ", function()

local args = {
    [1] = "Bonnie",
    [2] = "Outfit1"
}

game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("CharacterConfig"):WaitForChild("GetCharacter"):FireServer(unpack(args))




end)

