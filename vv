			if Player.Character ~= nil and Player ~= Plr then
				local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
				if mag < 20 then
					local Target = Player.Character or Player.CharacterAdded:Wait()
	
