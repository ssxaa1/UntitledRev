        if player then
            local pos = player.Character.HumanoidRootPart.CFrame

    local A_1 = "Magic"
    local A_2 = 
        {
            [1] = "Motus",
            [2] = true,
            [3] = player.Character
        }
    local Event = game:GetService("ReplicatedStorage").Events.Witch
    Event:FireServer(A_1, A_2)																		
        end
    end
