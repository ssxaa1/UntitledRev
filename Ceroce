    [3] = player.Character,
    [4] = Vector3.new(369.16180419921875, -11.344131469726562, 972.9254150390625),
    [5] = "Hold"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Realistic"):FireServer(unpack(args))

