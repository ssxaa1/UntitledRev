BennettSection:NewButton("Mass snap", "", function()

	local AnimationId = "6082221949"
	local Anim = Instance.new("Animation")
	Anim.AnimationId = "rbxassetid://"..AnimationId
	local k = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
	k:Play()
	k:AdjustSpeed(0.5)
local args = {
    [1] = "Menedek Qual Surenta!",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))

    local Plr = game.Players.LocalPlayer
    for i, Player in pairs (game.Players:GetPlayers()) do

        if Player.Character ~= nil and Player ~= Plr then
            local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
            if mag < 20 then
                local Target = Player.Character or Player.CharacterAdded:Wait()

	-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = "Drink",
    [2] = workspace.Builds.Rikkis.Part,
    [3] = Vector3.new(),
    [4] = Target.Head
}

game:GetService("ReplicatedStorage").System_Remotes.Tool:FireServer(unpack(bennettscripts))

            end
        end
end   
end)

