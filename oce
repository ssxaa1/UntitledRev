    local BennettSection = Bennett:NewSection("Removers abuse")

 
  BennettSection:NewTextBox("Remove player", "", function(txt)
    local Players = game:GetService("Players")
    local msg = txt
    local function findPlayer(stringg)
        for _, v in pairs(Players:GetPlayers()) do
            if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                return v
            end
        end
    end

    local player = findPlayer(msg)
    local CHAR = player.Character

-- Script generated by Bennett Spy - discord: bennettscripts
bennettscripts = {
    [1] = "Drink",
    [2] = workspace.Builds.Rikkis.Part,
    [3] = Vector3.new(),
    [4] = player.Character
}

game:GetService("ReplicatedStorage").System_Remotes.Tool:FireServer(unpack(bennettscripts))

end)

