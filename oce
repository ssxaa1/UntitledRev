                           local selectedOutfit;
                           selectedOutfit = currentOption;
                           if selectedOutfit then
local args = {
    [1] = selectedOutfit,
    [2] = "Rikki"
}

game:GetService("ReplicatedStorage"):WaitForChild("System_Remotes"):WaitForChild("CharacterChoose"):FireServer(unpack(args))
                           end
local oldList = {
  "Outfit1",
  "Outfit2",
   "Outfit3",
   "Outfit4"
  
}
local newList = {
  "Rikki"
}
                           wait(0.5)
                           dropdown:Refresh(newList)
    print(currentOption)
end)

